  <resultMap type="ScoreConfig" id="pageList" extends="BaseResultMap">
  	<collection property="scoreLevels" ofType="ScoreLevel">
  		 <result column="LEVEL_NAME" property="levelName" jdbcType="VARCHAR" />
    	<result column="SCORE_RANGE_MIN" property="scoreRangeMin" jdbcType="DECIMAL" />
    	<result column="SCORE_RANGE_MAX" property="scoreRangeMax" jdbcType="DECIMAL" />
  	</collection>
  </resultMap>
  <select id="setPageList" resultMap="pageList">
  	SELECT sc.ID,sc.`NAME`,sl.LEVEL_NAME,sl.SCORE_RANGE_MIN,sl.SCORE_RANGE_MAX,sc.STATUS
  		from score_config sc 
		join score_level sl on sl.SCORE_CONFIG_ID=sc.ID and sl.TYPE='LEVEL'
	WHERE sc.status!='DELETE'
		<if test="name !=null and name!=''">
			and sc.name like #{name}
		</if>
		<if test="status !=null and status!=''">
			and sc.status=#{status}
		</if>
	ORDER BY sc.CREATE_TIME DESC,sl.SCORE_RANGE_MAX DESC
  </select>
